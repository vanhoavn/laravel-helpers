{
    "name": "vanhoavn/laravel-helper",
    "description": "Helper commands for laravel projects.",
    "license": "MIT",
    "homepage": "https://vhn.vn/",
    "support": {
        "issues": "https://github.com/vanhoavn/laravel-helpers/issues",
        "source": "https://github.com/vanhoavn/laravel-helpers"
    },
    "keywords": [
        "laravel",
        "helper"
    ],
    "authors": [
        {
            "name": "vanhoa",
            "email": "vanhoa@vhn.vn"
        }
    ],
    "require": {
        "illuminate/support": "^5.8 || ^6.0 || ^7.0",
        "illuminate/container": "^5.8 || ^6.0",
        "illuminate/database": "^5.8 || ^6.0",
        "illuminate/events": "^5.8 || ^6.0",
        "php": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Vhnvn\\LaravelHelper\\": ""
        }
    },
    "suggest": {},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Vhnvn\\LaravelHelper\\ModulesServiceProvider"
            ]
        }
    }
}
